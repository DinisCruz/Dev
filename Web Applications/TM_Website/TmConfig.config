<?xml version="1.0"?>
<TMConfig>
  <UseAppDataFolder>false</UseAppDataFolder>
  <TMLibraryDataVirtualPath />
  <XmlLibrariesPath>TM_Libraries</XmlLibrariesPath>
  <DefaultAdminUserName>admin</DefaultAdminUserName>
  <DefaultAdminPassword>!!tmadmin</DefaultAdminPassword>
  <LibrariesUploadedFiles>TM_Library_UploadedFiles</LibrariesUploadedFiles>
  <ShowContentToAnonymousUsers>false</ShowContentToAnonymousUsers>
  <SSL_RedirectHttpToHttps>true</SSL_RedirectHttpToHttps>
  <SanitizeHtmlContent>false</SanitizeHtmlContent>
  <SingleSignOn_Enabled>true</SingleSignOn_Enabled>
  <REST>
    <AllowCrossDomainAccess>false</AllowCrossDomainAccess>
  </REST>
  <OnInstallation>
    <ForceAdminPasswordReset>false</ForceAdminPasswordReset>
    <DefaultLibraryToInstall_Name>Top Vulnerabilities</DefaultLibraryToInstall_Name>
    <DefaultLibraryToInstall_Location>https://github.com/TeamMentor/Library_Top_Vulnerabilities/zipball/master</DefaultLibraryToInstall_Location>
  </OnInstallation>
  <WindowsAuthentication>
    <Enabled>false</Enabled>
    <ReaderGroup>TEAMMENTOR\TeamMentor Readers</ReaderGroup>
    <EditorGroup>TEAMMENTOR\TeamMentor Editors</EditorGroup>
    <AdminGroup>TEAMMENTOR\TeamMentor Admins</AdminGroup>
  </WindowsAuthentication>
  <TMDebugAndDev>
    <EnableGZipForWebServices>false</EnableGZipForWebServices>
    <Enable302Redirects>false</Enable302Redirects>
  </TMDebugAndDev>
  <Eval_Accounts>
    <Enabled>true</Enabled>
    <Days>15</Days>
  </Eval_Accounts>
</TMConfig>